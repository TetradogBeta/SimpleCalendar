<Window x:Class="KawaiCalendar.DiaManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KawaiCalendar"
        mc:Ignorable="d"
        Title="DiaManagerWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="20"/>
        </Grid.RowDefinitions>
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <StackPanel x:Name="stkYears"/>
        </ScrollViewer>
        <Button x:Name="btnAdd" Grid.Row="1" Content="Add" Click="btnAdd_Click"/>

    </Grid>
</Window>
